{
  "extends": "airbnb-base",
  "rules": {
    "no-var": "off",
    "camelcase": "off",
    "func-names": "off",
    "vars-on-top": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-cond-assign": "off",
    "no-mixed-operators": "off",
    "prefer-arrow-callback": "off",
    "comma-dangle": "off",
    "linebreak-style": "off"
  },
  "globals": {
    "describe": true,
    "it": false
  }
}
